<keymap version="1" name="custom_keymap" parent="Default for XWin">
  <action id="ActivateMavenProjectsToolWindow">
    <keyboard-shortcut first-keystroke="alt f11" />
  </action>
  <action id="CommentByLineComment">
    <keyboard-shortcut first-keystroke="ctrl slash" />
    <keyboard-shortcut first-keystroke="ctrl divide" />
    <keyboard-shortcut first-keystroke="shift ctrl 7" />
  </action>
  <action id="ExpandAllRegions">
    <keyboard-shortcut first-keystroke="shift ctrl add" />
    <keyboard-shortcut first-keystroke="shift ctrl equals" />
    <keyboard-shortcut first-keystroke="shift ctrl plus" />
  </action>
  <action id="ExpandRegion">
    <keyboard-shortcut first-keystroke="ctrl add" />
    <keyboard-shortcut first-keystroke="ctrl equals" />
    <keyboard-shortcut first-keystroke="ctrl plus" />
  </action>
  <action id="Scala.ShowImplicits">
    <keyboard-shortcut first-keystroke="shift ctrl alt equals" />
    <keyboard-shortcut first-keystroke="shift ctrl alt add" />
  </action>
  <action id="Scala.TypeInfo">
    <keyboard-shortcut first-keystroke="alt #10000df" />
  </action>
  <action id="SearchEverywhere">
    <keyboard-shortcut first-keystroke="alt space" />
  </action>
  <action id="ToggleBookmark7" />
</keymap>